{
  "plugins": {
    "time": {
      "url": "oci://ghcr.io/tuananh/time-plugin:v0.1.8"
    },
    "fetch": {
      "url": "oci://ghcr.io/tuananh/fetch-plugin:v0.1.8",
      "runtime_config": {
        "allowed_hosts": [
          "*"
        ],
        "memory_limit": "100 MB"
      }
    },
    "context7": {
      "url": "oci://ghcr.io/tuananh/context7-plugin:v0.1.8",
      "runtime_config": {
        "allowed_hosts": [
          "context7.com"
        ]
      }
    },
    "think": {
      "url": "oci://ghcr.io/tuananh/think-plugin:v0.1.8"
    },
    "arxiv": {
      "url": "oci://ghcr.io/tuananh/arxiv-plugin:v0.1.8",
      "runtime_config": {
        "allowed_hosts": [
          "export.arxiv.org",
          "arxiv.org"
        ],
        "allowed_paths": [
          "/tmp"
        ]
      }
    }
  }
}
